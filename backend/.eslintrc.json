{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "no-param-reassign": "off"
    }
}
